    @Scripts.Render("~/Public/HamDungChung")
    @*---------------*@
    @* END SmartBus *@
    @*-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=*@

    <!-- Handler for local scripts -->
    @*----------------------------------------------*@
    @* Handler for local scripts *@
    @if (IsSectionDefined("Scripts"))
    {@RenderSection("Scripts", required: false)}

    <!--  -->
    @* BEGIN THEME LAYOUT SCRIPTS *@
    @Scripts.Render("../assets/layouts/layout/scripts/layout.js")
    @Scripts.Render("../assets/layouts/layout/scripts/layout.min.js")
    @Scripts.Render("../assets/layouts/layout/scripts/demo.min.js")
    @Scripts.Render("../assets/layouts/global/scripts/quick-sidebar.js")
    @Scripts.Render("../assets/layouts/global/scripts/quick-sidebar.min.js")
    @Scripts.Render("../assets/pages/scripts/task.js")
    @Scripts.Render("../assets/pages/scripts/task.min.js")

    @* END THEME LAYOUT SCRIPTS *@


</html>
