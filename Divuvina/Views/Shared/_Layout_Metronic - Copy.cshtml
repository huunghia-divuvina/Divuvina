    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("../assets/global/plugins/jquery-ui/jquery-ui.min.js")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("../assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js")
    @Scripts.Render("../assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js")
    @Scripts.Render("../assets/global/plugins/jquery.blockui.min.js")
    @Scripts.Render("../assets/global/plugins/js.cookie.min.js")
    @Scripts.Render("../assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js")
    @* END CORE PLUGINS *@
    <!--  -->
    @* BEGIN PAGE LEVEL PLUGINS *@
    @Scripts.Render("../assets/global/plugins/jqvmap/jqvmap/jquery.vmap.js")
    @Scripts.Render("../assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.russia.js")
    @Scripts.Render("../assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.world.js")
    @Scripts.Render("../assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.europe.js")
    @Scripts.Render("../assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.germany.js")
    @Scripts.Render("../assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.usa.js")
    @Scripts.Render("../assets/global/plugins/jqvmap/jqvmap/data/jquery.vmap.sampledata.js")
    @Scripts.Render("../assets/global/plugins/flot/jquery.flot.min.js")
    @Scripts.Render("../assets/global/plugins/flot/jquery.flot.resize.min.js")
    @Scripts.Render("../assets/global/plugins/flot/jquery.flot.categories.min.js")
    @Scripts.Render("../assets/global/plugins/jquery.pulsate.min.js")
    @Scripts.Render("../assets/global/plugins/bootstrap-daterangepicker/moment.min.js")
    @Scripts.Render("../assets/global/plugins/bootstrap-daterangepicker/daterangepicker.min.js")
    @*-- IMPORTANT! fullcalendar depends on jquery-ui-1.10.3.custom.min.js for drag & drop support --*@
    @Scripts.Render("../assets/global/plugins/fullcalendar/fullcalendar/fullcalendar.min.js")
    @Scripts.Render("../assets/global/plugins/jquery-easypiechart/jquery.easypiechart.min.js")
    @Scripts.Render("../assets/global/plugins/jquery.sparkline.min.js")
    @*@Scripts.Render("../assets/global/plugins/gritter/js/jquery.gritter.js")*@
    @Scripts.Render("../assets/global/plugins/morris/morris.min.js")
    @Scripts.Render("~/plugins/toastr")
    @* END PAGE LEVEL PLUGINS *@
    <!--  -->
    @* BEGIN THEME GLOBAL SCRIPTS *@
    @Scripts.Render("~/assest/global/scripts")
    @Scripts.Render("~/assest/apps/scripts")
    @* END THEME GLOBAL SCRIPTS *@
    <!--  -->
    @* BEGIN PAGE LEVEL SCRIPTS *@
    @Scripts.Render("../assets/global/plugins/moment.min.js")
    @Scripts.Render("~/assest/pages/scripts")
    @* END PAGE LEVEL SCRIPTS *@
    <!--  -->
    @**@
    @**@
    <!--  -->

    @* END METRONIC *@
    @*-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=*@


    @* Jquery Validate *@
    @Scripts.Render("~/plugins/validate")

    @* START SmartBus *@
    @*----------------------------------------------*@
    @* Hàm dùng Validation Form *@
    @*@Scripts.Render("~/plugins/nghuuphuoc-bootstrapvalidator-v0.5.2")*@
